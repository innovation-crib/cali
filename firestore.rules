rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /bookings/{bookingId} {
      allow read: if true;
      allow create: if request.auth.uid != null && request.resource.data.userId == request.auth.userId;
    }


    match /{document=**} {
      allow read, write: if false;
    }
  }
}